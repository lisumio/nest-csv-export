{
	"compilerOptions": {
	  "module": "commonjs",
	  "declaration": true,
	  "removeComments": true,
	  "emitDecoratorMetadata": true,
	  "experimentalDecorators": true,
	  "allowSyntheticDefaultImports": true,
	  "target": "es2019",
	  "sourceMap": true,
	  "outDir": "./dist",
	  "baseUrl": ".",
	  "incremental": true,
	  "skipLibCheck": true,
	  "lib": ["es2019"],
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "paths": {
		"@app/*": ["./src/*"]
	  }
	}
  }
  